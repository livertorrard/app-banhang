        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Thongtin" />
        <activity android:name=".LienHe" />
        <activity android:name=".Nike" />
        <activity android:name=".Adias" />
        <activity android:name=".ThongTinKhachHang" />
        <activity android:name=".GioHang" />
        <activity android:name=".Chitietsanpham" />
        <activity android:name=".SplashHome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>